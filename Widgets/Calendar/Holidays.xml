<!--适用于中国地区。暂无法添加农历节日。-->
 <EventFile Repeat="Year">
 	<Event Month="{$EasterMonth}" Day="{$EasterDay}" Description="复活节"/>
	<Set Month="1">
		<Event Day="1" Description="元旦"/>
	</Set>
	<Set Month="2">
		<Event Day="7" Description="二七纪念日"/>
